{
  "name": "fifa_worldcup",
  "version": "1.0.0",
  "description": "A web application for predicting FIFA World Cup match outcomes and creating your perfect tournament bracket. Similar to March Madness brackets, but for soccer!",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "postcss ./static/css/tailwind.input.css -o ./static/css/tailwind.output.css",
    "watch:css": "postcss ./static/css/tailwind.input.css -o ./static/css/tailwind.output.css --watch",
    "build:css:prod": "NODE_ENV=production postcss ./static/css/tailwind.input.css -o ./static/css/tailwind.output.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germarr/fifaworldcup2026.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/germarr/fifaworldcup2026/issues"
  },
  "homepage": "https://github.com/germarr/fifaworldcup2026#readme",
  "dependencies": {
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  }
}
